{
    "manifest_version": 3,
    "name": "Awesome Tab",
    "description": "Boost up your mind using Awesome Tab whenever you open your browser!",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "34.png"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "edge_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "128": "128.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["content.styles.css", "128.png", "34.png"],
            "matches": []
        }
    ],
    "version": "0.1",
    "version_name": "0.1-beta"
}
